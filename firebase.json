{
  "hosting": {
    "site": "zairtre-music",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/api/**",
        "run": {
          "serviceId": "zairtre-cloudrun",
          "region": "us-central1"
        }
      },
      {
        "source": "**",
        "run": {
          "serviceId": "zairtre-cloudrun",
          "region": "us-central1"
        }
      }
    ]
  }
}
